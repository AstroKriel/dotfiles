
[
  { "key": "cmd+shift+o", "command": "workbench.action.quickOpen" },
  { "key": "cmd+alt+o", "command": "workbench.action.quickOpen" },
  { "key": "cmd+alt+i", "command": "workbench.action.gotoLine" },
  // focus editor
  { "key": "cmd+shift+.", "command": "workbench.action.focusActiveEditorGroup" },
