  { "key": "cmd+shift+d", "command": "workbench.action.closeOtherEditors" },
  { "key": "cmd+alt+d", "command": "workbench.action.closeEditorsToTheRight" },
  { "key": "cmd+d", "command": "workbench.action.closeActiveEditor" },
  // editor (group) navigation
  { "key": "cmd+alt+h", "command": "workbench.action.focusLeftGroup" },
