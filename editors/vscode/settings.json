{
  "editor.fontSize": 10,
  "markdown.preview.fontSize": 11,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "editor.renderWhitespace": "boundary",
  "editor.guides.indentation": true,
  "editor.guides.highlightActiveIndentation": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": "active",
  "search.seedWithSelection": true,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.scrollBeyondTop": false,
  "editor.scrollBeyondLastLine": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "files.autoSave": "afterDelay",
  "gitlens.advanced.messages.suppressGitVersionWarning": true,
  "gitlens.codeLens.includeSingleLineSymbols": false,
  "gitlens.heatmap.locations": [
    "gutter",
    "overview"
  ],
  "editor.minimap.enabled": true,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fill",
  "editor.minimap.renderCharacters": true,
  "editor.minimap.maxColumn": 100,
  "python.languageServer": "Pylance",
  "python.terminal.activateEnvironment": false,
  "remote.SSH.showLoginTerminal": true,
  "remote.SSH.logLevel": "trace",
  "remote.SSH.experimental.chat": false,
  "terminal.integrated.scrollback": 50000,
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "window.autoDetectColorScheme": true,
  "workbench.preferredLightColorTheme": "Solarized Light",
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "workbench.startupEditor": "none",
  "workbench.panel.defaultLocation": "left",
  "workbench.sideBar.location": "right",
  "explorer.sortOrder": "type",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "workbench.editor.revealIfOpen": true,
  "workbench.tree.indent": 25,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.enablePreview": false,
  "workbench.navigationControl.enabled": false,
  "window.commandCenter": false
}