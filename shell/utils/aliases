## ls
alias lsl='ls -lhF --color=auto'
alias lst='ls -lt --color=auto'
alias lsa='ls -alhF --color=auto'

## shortcut
alias da='deactivate'

## useful directories
export DHOME="$HOME"
alias dhome='cd "$DHOME"'
alias dcodes='cd "$DHOME/Documents/Codes"'
alias ddwn='cd "$DHOME/Downloads"'
alias done='cd "$DHOME/OneDrive"'

## .